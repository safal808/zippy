  <title>Download File</title>
  </head>
  <body>
    <div class="wrapper">
      <h2>Download <%= file.originalName %></h2>
      <br>
      <p>Are you sure you want to download <%= file.originalName %>?</p>
      <br>
      <a href="/file/<%= file.id %>/download" download>
          <button><i class="fa fa-download" aria-hidden="true" style="color: black;"></i>
            Download</button>   </a>
    </div>

   
  
  </body>

</html>